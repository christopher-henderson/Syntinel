[Unit]
Description=The Syntinel world state server and test execution load balancers and scheduler.
After=syslog.target network.target remote-fs.target nss-lookup.target

[Service]
Type=forking
PIDFile=/opt/download_server/uwsgi/uwsgi.pid
ExecStart=/usr/bin/env uwsgi --ini /opt/Syntinel/compozed/uwsgi/uwsgi.ini
ExecReload=/bin/kill -s HUP $MAINPID
ExecStop=/bin/kill -s QUIT $MAINPID
PrivateTmp=true

[Install]
WantedBy=multi-user.target
